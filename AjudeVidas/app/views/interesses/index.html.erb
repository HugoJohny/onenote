<p id="notice"><%= notice %></p>

<h1>Interesses</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Produto</th>
      <th>Pessoa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interesses.each do |interess| %>
      <tr>
        <td><%= interess.status %></td>
        <td><%= interess.produto %></td>
        <td><%= interess.pessoa %></td>
        <td><%= link_to 'Show', interess %></td>
        <td><%= link_to 'Edit', edit_interess_path(interess) %></td>
        <td><%= link_to 'Destroy', interess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interess', new_interess_path %>
